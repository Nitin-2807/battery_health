{
  "version": 2,
  "name": "battery-health-predictor",
  "builds": [
    {
      "src": "api/predict.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/predict.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/predict.py": {
      "maxDuration": 10
    }
  },
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}